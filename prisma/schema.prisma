generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Order{
  id                 String      @id @default(uuid()) @db.Uuid
  status             Status_Order      @default(CLEANING)
  room_number        String
  x                  Float
  y                  Float
  order_type         Order_Type      @default(FULL)
}

enum Status_Order{
  CLEANING
  COMPLETED
}

enum Order_Type{
  FULL
  TECHNICAL
}
